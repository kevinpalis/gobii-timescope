<?link rel="stylesheet" type="text/css" href="/style.css"?>
<?init class="org.gobiiproject.datatimescope.services.AuthenticationInit"?>
<zk>

    <style>
            .mainText * { 
            font-size: 28px !important;  
        } 
    </style>
	<borderlayout hflex="1" vflex="max" apply="org.gobiiproject.datatimescope.controller.BookmarkChangeController">
		<north height="100px" border="none" >
			<include src="banner.zul"/>
		</north>
		<west width="260px" collapsible="true" splittable="true" minsize="300" border="0">
			<include src="sidebar.zul"/>
		</west>
		<center autoscroll="true" vflex="true" border="0">
			<include id="mainContent" src="mainContent.zul"/>
		</center>
		<south height="70px" border="none">
			<include src="footer.zul"/>
		</south>
	</borderlayout>
</zk>